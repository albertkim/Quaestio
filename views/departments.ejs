<% include partials/header %>

	<h3>School: </h3>
	<a href="<%= schoolName %>"><%= schoolName %></a>
	
	<h3>Courses: </h3>

	<% for(var i=0; i<departments.length; i++){ %>
		<h3><%= departments[i].departmentId %></h3>
		<% for(var j=0; j<departments[i].courses.length; j++){ %>
			<li><a href="/schools/<%= departments[i].courses[j].SCHOOLNAME %>/<%=departments[i].courses[j].ID%>"><%= departments[i].courses[j].NAME %></a></li>
		<% } %>
	<% } %>

<% include partials/footer %>